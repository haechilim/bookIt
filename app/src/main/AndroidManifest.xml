<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BookIt">
        <activity android:name=".activity.ChattingActivity"></activity>
        <activity android:name=".activity.WriteReadingDiaryActivity" />
        <activity android:name=".activity.WriteMarketActivity" />
        <activity android:name=".activity.WriteDebateActivity" />
        <activity android:name=".activity.DebateDetailActivity" />
        <activity android:name=".activity.BookDetailActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>